{
  "name": "swa-auth",
  "version": "1.0.0",
  "description": "A super-lightweight Auth library for Azure Static Web Apps.",
  "main": "dist/auth.js",
  "types": "dist/auth.d.ts",
  "repository": "git@github.com:tcc-sejohnson/swa-auth.git",
  "keywords": ["Azure", "Static Web Apps", "Auth"],
  "author": "S. Elliott Johnson",
  "license": "GNU GPL V3",
  "bugs": {
    "url": "https://github.com/tcc-sejohnson/swa-auth/issues"
  },
  "homepage": "https://github.com/tcc-sejohnson/swa-auth/issues#readme",
  "private": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run",
    "preversion": "npm run lint",
    "version": "npm run format && git add --all src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.30",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5"
  }
}
